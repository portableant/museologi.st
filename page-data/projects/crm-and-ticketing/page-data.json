izes":"(min-width: 1200px) 1200px, 100vw"},{"srcSet":"/static/38fbd5e7a0d15455ff0926fa4c24c14f/d863a/25th_november_131_2000x1000.webp 300w,\n/static/38fbd5e7a0d15455ff0926fa4c24c14f/3879c/25th_november_131_2000x1000.webp 600w,\n/static/38fbd5e7a0d15455ff0926fa4c24c14f/0429b/25th_november_131_2000x1000.webp 1200w","type":"image/webp","sizes":"(min-width: 1200px) 1200px, 100vw"}]},"width":1200,"height":600}}},"project_website":"https://tickets.museums.cam.ac.uk","slug":"/projects/crm-and-ticketing","vimeo":null,"zenodo_doi":null,"github_repo":[{"url":"https://github.com/FitzwilliamMuseum/tessitura-fitz","name":"Tessitura Fitz Jekyll front end"}],"type":null,"grant_number":null,"grant_amount":null,"funder":null,"funders":["Fitzwilliam Museum"],"tags":["museums","creative economy","ticketing","CRM","events","pandemic digital"],"deposited_archive":null,"documents":null,"audio":null,"images":null}}},"pageContext":{"slug":"/projects/crm-and-ticketing","id":"4f859fa0-5dfd-5c8d-9be2-8e56c27a6a79"}},"staticQueryHashes":["3600676621","4037206124"]}